 - tasks for dealing with the project file.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Edit"

\end_inset

 - tasks for undoing and redoing tasks in the schematic.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Parts"

\end_inset

 - tasks for dealing with schematic elements in the drawing.
\end_layout

\begin_layout Itemize
