	<Equations>
		<AutoDebug>True</AutoDebug>
		<Lines>
			<EquationLine>
				<Line>a2080=1.683244 # hardcoded relationship that produces 20-80 risetime</Line>
			</EquationLine>
			<EquationLine>
				<Line>a1090=2.563103 # hardcoded relationship that produces 10-90 risetime</Line>
			</EquationLine>
			<EquationLine>
				<Line>a = a1090 if rt_type == '1090' else a2080</Line>
			</EquationLine>
			<EquationLine>
				<Line></Line>
			</EquationLine>
			<EquationLine>
				<Line>tx_filter=f'math.exp(-2*(math.pi*f*{rt}/{a})**2)'</Line>
			</EquationLine>
			<EquationLine>
				<Line></Line>
			</EquationLine>
			<EquationLine>
				<Line>ImpulseResponseLength = rt*1.2*10*2</Line>
			</EquationLine>
			<EquationLine>
				<Line></Line>
			</EquationLine>
			<EquationLine>
				<Line>import SignalIntegrity.App.Project</Line>
			</EquationLine>
			<EquationLine>
				<Line>import math</Line>
			</EquationLine>
			<EquationLine>
				<Line>proj=SignalIntegrity.App.Project</Line>
			</EquationLine>
			<EquationLine>
				<Line>frequency_resolution=1/ImpulseResponseLength</Line>
			</EquationLine>
			<EquationLine>
				<Line>end_frequency=proj['CalculationProperties.EndFrequency']</Line>
			</EquationLine>
			<EquationLine>
				<Line>frequency_points=int(max(2,math.ceil(end_frequency/frequency_resolution)))</Line>
			</EquationLine>
			<EquationLine>
				<Line>proj['CalculationProperties.FrequencyPoints']=frequency_points</Line>
			</EquationLine>
			<EquationLine>
				<Line>proj['CalculationProperties'].CalculateOthersFromBaseInformation()</Line>
			</EquationLine>
			<EquationLine>
				<Line>ImpulseResponseLength = proj['CalculationProperties.ImpulseResponseLength']</Line>
			</EquationLine>
			<EquationLine>
				<Line></Line>
			</EquationLine>
