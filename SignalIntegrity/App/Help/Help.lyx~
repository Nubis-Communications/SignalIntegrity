\begin_inset Text

\begin_layout Plain Layout

\emph on
Menu System
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset CommandInset ref
LatexCommand nameref
reference "subsec:Parts"

\end_inset

