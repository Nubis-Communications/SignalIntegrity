LatexCommand nameref
reference "Control-Help:S-Parameter-Viewer-Open-Help-File"

\end_inset

 - opens the help file to the 
\begin_inset CommandInset ref
LatexCommand nameref
reference "sec:S-parameter-Viewer"

\end_inset

 section.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "Control-Help:S-Parameter-Viewer-Control-Help"

\end_inset

 - for help on any menu elements or toolbar buttons.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "Control-Help:S-Parameter-Viewer-Preferences"

\end_inset

 - for editing s-parameter viewer preferences.
\end_layout

