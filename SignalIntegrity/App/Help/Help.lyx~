\end_inset

 - for help on any menu elements or toolbar buttons.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "sec:Preferences"

\end_inset

 - for editing any global preferences.
\end_layout

