\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Attaching-Ports-to-a-Schematic-for-Deembedding"

\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Defining-the-System"

\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Setting-Calculation-Properties-for-Deembedding"

\end_inset

.
\end_layout

\begin_layout Itemize
Running the 
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Deembedding-Calculation"

\end_inset

.
\end_layout

\begin_layout Standard
If you like, you can read the 
\begin_inset CommandInset ref
LatexCommand nameref
reference "sub:Deembedding-Example"

\end_inset

 where hopefully everything will be clear.
\end_layout

