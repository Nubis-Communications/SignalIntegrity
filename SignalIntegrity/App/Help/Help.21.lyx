\begin_inset Formula 
\[
V_{d}=\frac{V_{+}-V_{-}}{\sqrt{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
V_{c}=\frac{V_{+}+V_{-}}{\sqrt{2}}
\]

\end_inset


\end_layout

